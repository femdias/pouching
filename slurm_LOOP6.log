.                 
.                 // wage in t=-12, level
.                 
.                 gen wage_real_l12_temp = wage_real if ym_rel == -12
 16.                 egen wage_real_l12 = max(wage_real_l12_temp), by(cpf)
 17.                 
.                 drop wage_real_l12_temp
