>                 
>                 use "${data}/rais_m/rais_m`d_ym'", clear
>                 
>                 merge m:1 plant_id using "${temp}/202503_d_plant_`ym'"
>                 keep if _merge == 3
>                 drop _merge
>                 
>                 keep cpf eventid pc_ym
>                 
>                 compress
>                 save "${temp}/202503_d_cw_`ym'", replace
